// "manifest_version": 2,
{
  "name": "VGA Planets Assistant",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Ziplist's VGA Planets Extension",
  "browser_action": {
    "default_icon" : "icon.png",
    "default_popup" : "config.html",
    "background" :  {
      "scripts" : ["background.js"]
    }
  },
  "permissions": [
    "tabs", "http://*/*",
    "storage"
  ]
}
